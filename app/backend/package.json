{
  "name": "backend-todolist",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server",
  "scripts": {
    "build": "/bin/sh tsc_eval.sh",
    "test": "mocha -r ts-node/register ./src/tests/*$NAME*/*$NAME*.{test,spec}.ts -t 10000 --exit",
    "prestart": "npm run build && npm run db:reset",
    "start": "node .",
    "dev": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "eslint ./src --ext .ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-http": "^4.2.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.5.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.45",
    "@types/sequelize": "^4.28.13",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-sonarjs": "^0.13.0",
    "jest": "^28.1.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "superagent": "^7.1.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.2",
    "sequelize-cli": "^6.4.1",
    "ts-node-dev": "^1.1.8",
    "typeorm": "^0.3.7"
  }
